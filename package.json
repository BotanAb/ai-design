{
  "name": "gatsby-theme-carbon-starter",
  "version": "0.0.5",
  "main": "src",
  "repository": {
    "url": "git@github.com:carbon-design-system/gatsby-theme-carbon.git",
    "type": "git"
  },
  "author": "vpicone <vp@vincepic.one> (@vpicone)",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "trash .cache && rm -rf public && PATH_PREFIX='/design/ai' gatsby build --prefix-paths && mv public ai && mkdir design && mv ai design && mkdir public && mv design public && cp ./static/hero.png ./static/hero.mp4 public/design/ai",
    "develop:clean": "trash .cache && gatsby develop",
    "prod": "npm run build && gatsby serve",
    "now-build": "npm run build"
  },
  "dependencies": {
    "gatsby": "^2.15.14",
    "gatsby-plugin-google-analytics": "^2.1.19",
    "gatsby-theme-carbon": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "gatsby-plugin-force-trailing-slashes": "^1.0.4"
  }
}
